{
  "name": "ldpos-postgres-dal",
  "version": "1.0.0",
  "description": "Postgres Data Access Layer for LDPoS",
  "main": "index.js",
  "scripts": {
    "test:integration": "mocha test/integration --reporter spec --timeout 10000 --slow 10000",
    "migrate:make": "knex migrate:make",
    "migrate": "knex migrate:latest",
    "migrate:rollback": "knex migrate:rollback",
    "seed:make": "knex seed:make",
    "seed": "knex seed:run",
    "start:db": "docker-compose up db",
    "stop:db": "docker-compose stop db && docker-compose rm -f db",
    "start:pgadmin": "docker-compose up pgadmin",
    "stop:pgadmin": "docker-compose stop pgadmin && docker-compose rm -f pgadmin"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Leasehold/ldpos-postgres-dal.git"
  },
  "keywords": [
    "ldpos",
    "postgres",
    "dal"
  ],
  "author": "Jonathan Gros-Dubois",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Leasehold/ldpos-postgres-dal/issues"
  },
  "homepage": "https://github.com/Leasehold/ldpos-postgres-dal#readme",
  "devDependencies": {
    "mocha": "^8.2.1"
  },
  "dependencies": {
    "knex": "^0.21.16",
    "pg": "^8.5.1"
  }
}
